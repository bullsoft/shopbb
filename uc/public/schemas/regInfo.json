{
    "title": "regInfo",
    "type": "object",
    "properties": {
        "username": {
            "type": "string",
            "minLength": 5
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "captcha": {
            "type": "string",
            "pattern": "^([0-9A-Za-z]{5})?$"
        },
        "mobile": {
            "type": "string",
            "minLength": 11,
            "maxLength": 11
        },
        "passwd": {
            "type": "string",
            "minLength": 6
        }
    },
   
    "required": ["username", "passwd"]
    
}